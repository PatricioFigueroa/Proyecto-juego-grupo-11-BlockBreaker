     };&#xD;
&#xD;
     blocks.clear();&#xD;
     int blockWidth = Gdx.graphics.getWidth() / 13 - 10;&#xD;
     int blockHeight = Gdx.graphics.getHeight() / 20;&#xD;
     int y = Gdx.graphics.getHeight();&#xD;
     for (int i = 0; i &lt; height; i++) {&#xD;
