            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_3ftElXHCEe632Pb3buVU9w" name="dibujaTextos" specification="_3deP8HHCEe632Pb3buVU9w">
              <language>java</language>
              <body>// Actualizar matrices de la cámara&#xD;
     camera.update();&#xD;
     // Actualizar&#xD;
     batch.setProjectionMatrix(camera.combined);&#xD;
     batch.begin();&#xD;
&#xD;
     // Dibujar textos&#xD;
     font.draw(batch, &quot;Puntos: &quot; + controlador.getPuntaje(), 10, 25);&#xD;
     font.draw(batch, &quot;Vidas : &quot; + controlador.getVidas(), Gdx.graphics.getWidth() - 20, 25);&#xD;
     batch.end();&#xD;
 </body>
